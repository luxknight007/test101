package test;

public class FrogJmp {

	public static void main(String[] args) {
		// TODO Auto-generated method stub
		System.err.println(solution(10, 85, 30));
	}
	
	public static int solution (int X, int Y, int D) {
		double sum = (Y - X) / D;
		int jump = (int) Math.ceil(sum);
		return jump;
	}
}
